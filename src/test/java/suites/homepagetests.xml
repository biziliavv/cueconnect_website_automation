<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Home page tests" parallel="classes" thread-count="1">
    <test name="Positive testing ">
        <groups>
            <run>
                <include name="positive"/>
            </run>
        </groups>
        <classes>
            <class name="tests.HomePageTests"/>
        </classes>
    </test>
    <test name="Negative testing ">
        <groups>
            <run>
                <include name="negative"/>
            </run>
        </groups>
        <classes>
            <class name="tests.HomePageTests"/>
        </classes>
    </test>
</suite>